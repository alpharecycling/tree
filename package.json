{
  "name": "@alpharecycling/rc-tree",
  "version": "5.3.6-3",
  "description": "tree ui component for react",
  "engines": {
    "node": ">=10.x"
  },
  "keywords": [
    "react",
    "react-component",
    "react-tree",
    "tree"
  ],
  "files": [
    "assets/*.css",
    "assets/*.png",
    "assets/*.gif",
    "assets/*.less",
    "dist",
    "es",
    "lib"
  ],
  "homepage": "http://github.com/react-component/tree",
  "author": "smith3816@gmail.com",
  "repository": {
    "type": "git",
    "url": "git@github.com:alpharecycling/tree.git"
  },
  "bugs": {
    "url": "http://github.com/alpharecycling/tree/issues"
  },
  "license": "MIT",
  "main": "./lib/index",
  "module": "./es/index",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@alpharecycling"
  },
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "compile": "father build && lessc assets/index.less assets/index.css",
    "prepublishOnly": "npm run compile && np prerelease --yolo --no-publish --any-branch",
    "lint": "eslint src/ --ext .tsx,.ts,.jsx,.js",
    "test": "father test",
    "coverage": "father test --coverage",
    "now-build": "npm run docs:build",
    "prepare": "npm run compile"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.9",
    "@types/jest": "^26.0.4",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/warning": "^3.0.0",
    "@umijs/fabric": "^2.4.8",
    "dumi": "^1.1.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.0.0",
    "eslint": "^7.0.0",
    "father": "^2.29.8",
    "glob": "^7.1.6",
    "less": "^3.11.1",
    "np": "^6.0.0",
    "rc-dialog": "^8.1.0",
    "rc-tooltip": "5.x",
    "rc-trigger": "^5.0.7",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.1",
    "classnames": "2.x",
    "rc-motion": "^2.0.1",
    "rc-util": "^5.16.1",
    "rc-virtual-list": "^3.4.1"
  }
}
